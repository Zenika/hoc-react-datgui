{
  "name": "hoc-react-datgui-examples",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "set NODE_ENV=development && webpack-dev-server --inline --port=3000 --history-api-fallback",
    "build": "NODE_ENV=production webpack --define process.env.NODE_ENV='\"production\"' -p ",
    "lint": "eslint src --ext=jsx --ext=js -v --no-exit {} +",
    "publish": "npm run build && node publish"
  },
  "devDependencies": {
    "babel-core": "~6.7.5",
    "babel-eslint": "~6.0.0",
    "babel-loader": "~6.2.4",
    "babel-plugin-syntax-class-properties": "~6.5.0",
    "babel-plugin-transform-class-properties": "~6.6.0",
    "babel-plugin-transform-object-rest-spread": "~6.6.5",
    "babel-polyfill": "~6.7.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2017": "^1.4.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "~6.5.0",
    "babel-register": "~6.7.2",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "~0.8.5",
    "react-hot-loader": "~1.3.0",
    "style-loader": "~0.13.1",
    "webpack": "~1.12.2",
    "webpack-dev-server": "~1.12.1"
  },
  "dependencies": {
    "hoc-react-datgui": "^0.0.1",
    "react": "15.3.0",
    "react-dom": "15.3.0"
  },
  "babel": {
    "presets": [
      "es2017",
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb"
    ],
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "arrow-body-style": 0,
      "import/no-named-as-default": 0,
      "import/prefer-default-export": 0,
      "import/no-unresolved": 0,
      "react/forbid-prop-types": 0
    }
  }
}